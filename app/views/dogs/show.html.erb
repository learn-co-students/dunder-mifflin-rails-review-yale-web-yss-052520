<h4>Name: <%= @dog.name%></h4>
<h4>Breed: <%= @dog.breed%></h4>
<h4>Age: <%= @dog.age%></h4>
<h4>Employees: </h4>

<ul>
<% @dog.employees.each do |employee| %>
    <li>
        <%= link_to employee.name, "/employees/#{employee.id}"%>
    </li>
<% end %>
</ul>

<form method="get" action="/dogs">
    <button type="submit">Return to All the Dogs!</button>
</form>